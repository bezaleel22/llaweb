{
  "name": "llaweb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:static": "cp svelte.config.static.js svelte.config.js && vite build && git checkout svelte.config.js",
    "preview": "vite preview",
    "preview:static": "SVELTE_CONFIG=svelte.config.static.js vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "mdsvex": "0.12.3",
    "svelte": "^4.2.7",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "marked": "^14.1.3"
  }
}